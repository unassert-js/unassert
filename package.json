{
  "name": "unassert",
  "description": "Encourages programming with assertions by providing tools to compile them away",
  "version": "2.0.2",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "bugs": "https://github.com/unassert-js/unassert/issues",
  "contributors": [
    {
      "name": "Ren√©e Kooi",
      "url": "https://github.com/goto-bus-stop"
    },
    {
      "name": "Jordan Harband",
      "url": "https://github.com/ljharb"
    }
  ],
  "dependencies": {
    "estraverse": "^5.0.0",
    "magic-string": "^0.30.0"
  },
  "devDependencies": {
    "@twada/benchmark-commits": "^0.1.0",
    "acorn": "^8.0.0",
    "escodegen": "^2.0.0",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "src",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "exports": {
    ".": [
      {
        "import": "./src/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.cjs"
      },
      "./dist/index.cjs"
    ],
    "./package.json": "./package.json"
  },
  "homepage": "https://github.com/unassert-js/unassert",
  "keywords": [
    "DbC",
    "unassert",
    "assert",
    "assertion"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unassert-js/unassert.git"
  },
  "scripts": {
    "preversion": "npm run build && npm test",
    "build": "rimraf dist && rollup -c",
    "lint": "semistandard --verbose src/*.mjs test/*.mjs | snazzy",
    "fmt": "semistandard --fix src/*.mjs test/*.mjs",
    "test": "npm run lint && mocha test"
  },
  "semistandard": {
    "globals": [
      "describe",
      "beforeEach",
      "it"
    ]
  }
}
